# Copy this file to .env and fill with your actual values
CLOUDFLARE_API_TOKEN=your_cloudflare_api_token_here

# For R2 backend (optional, only if using R2 backend)
AWS_ACCESS_KEY_ID=your_r2_access_key_id
AWS_SECRET_ACCESS_KEY=your_r2_secret_access_key
AWS_EC2_METADATA_DISABLED=true

# For OVH provider
OVH_APPLICATION_KEY=your_ovh_application_key
OVH_APPLICATION_SECRET=your_ovh_application_secret
OVH_CONSUMER_KEY=your_ovh_consumer_key
OVH_ENDPOINT=ovh-eu

# For Infomaniak provider
INFOMANIAK_TOKEN=your_infomaniak_api_token

# For Terraform Cloud backend (optional)
TF_TOKEN_app_terraform_io=your_terraform_cloud_token

# For AWS S3 backend (optional)
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_DEFAULT_REGION=us-east-1

# For Azure Blob Storage backend (optional)
ARM_CLIENT_ID=your_azure_client_id
ARM_CLIENT_SECRET=your_azure_client_secret
ARM_SUBSCRIPTION_ID=your_azure_subscription_id
ARM_TENANT_ID=your_azure_tenant_id

# For Google Cloud Storage backend (optional)
GOOGLE_APPLICATION_CREDENTIALS=path/to/service-account.json
GOOGLE_CLOUD_PROJECT=your-project-id
GOOGLE_REGION=us-central1